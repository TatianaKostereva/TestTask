{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "last 2 Chrome versions",
        "node": "current"
      }
    }],
    [
      "@babel/preset-react",
      {
        "throwIfNamespace": false,
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["syntax-jsx"]
  ]
}